<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="700.0" minWidth="1240.0" prefHeight="454.0" prefWidth="405.0" style="-fx-background-color: #C4FCF0;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <Button layoutX="400.0" layoutY="300.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="500.0" style="-fx-background-color: #005575;" text="EDIT ITEMS" textFill="WHITE">
            <font>
                <Font name="Arial Black" size="14.0" />
            </font>
        </Button>
        <Button layoutX="400.0" layoutY="400.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="500.0" style="-fx-background-color: #005575;" text="DELETE ITEMS" textFill="WHITE">
            <font>
                <Font name="Arial Black" size="14.0" />
            </font>
        </Button>
        <Button layoutX="400.0" layoutY="200.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="500.0" style="-fx-background-color: #005575;" text="ADD ITEMS" textFill="WHITE" onAction="#AddItem">
            <font>
                <Font name="Arial Black" size="14.0" />
            </font>
        </Button>
      <Button fx:id="back" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goLogin" prefHeight="46.0" prefWidth="121.0" style="-fx-background-color: #C4FCF0;" text="&lt;- BACK" textAlignment="JUSTIFY" textFill="#005575">
         <font>
            <Font name="System Bold" size="21.0" />
         </font></Button>
    </children>
</AnchorPane>
